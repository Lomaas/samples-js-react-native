  },
  "devDependencies": {
    "babel-preset-react-native": "^4.0.0",
    "colors": "^1.3.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest-expo": "33.0.0",
    "react-dom": "^16.8.6",
    "react-test-renderer": "16.8.6"
  }
